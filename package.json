{
  "name": "@backtrack/preset-node-module",
  "version": "0.1.0",
  "description": "backtrack preset to bootstrap a node module",
  "keywords": [
    "backtrack",
    "preset",
    "node",
    "module",
    "babel",
    "flow"
  ],
  "license": "ISC",
  "author": "Chris Blossom",
  "repository": "chrisblossom/backtrack-preset-node-module",
  "homepage": "https://github.com/chrisblossom/backtrack-preset-node-module",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.10",
    "yarn": ">=1.6.0"
  },
  "files": [
    "lib/"
  ],
  "main": "lib/preset-node-module.js",
  "scripts": {
    "format": "backtrack format",
    "lint": "backtrack lint",
    "lint.fix": "backtrack lint.fix",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run lint",
    "prepush": "npm run lint"
  },
  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "backtrack": {
    "presets": [
      "@backtrack/preset"
    ]
  },
  "devDependencies": {
    "@backtrack/core": "^0.1.9",
    "@backtrack/preset-preset": "^0.1.5"
  },
  "dependencies": {
    "@backtrack/preset-jest": "^0.1.3",
    "@backtrack/preset-style": "^0.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.72.0",
    "flow-copy-source": "^1.3.0",
    "read-pkg-up": "^3.0.0"
  }
}
